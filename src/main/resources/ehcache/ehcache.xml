<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.ehcache.org/v3"
	xmlns:jcache="http://www.ehcache.org/v3/jsr107"
	xmlns:tc='http://www.ehcache.org/v3/clustered'
	xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.1.xsd
        http://www.ehcache.org/v3/jsr107 http://www.ehcache.org/schema/ehcache-107-ext-3.1.xsd http://www.ehcache.org/v3/clustered http://www.ehcache.org/schema/ehcache-clustered-ext-3.1.xsd">

	<service>
		<jcache:defaults default-template="default"
			enable-management="false" enable-statistics="false" />
	</service>
	<!-- OPTIONAL A <cache-template> defines a named template that can be used 
		be <cache> definitions in this same file They have all the same property 
		as the <cache> elements above -->
	<cache-template name="default">
		<expiry>
			<ttl unit="seconds">30</ttl>
		</expiry>
		<resources>
			<heap unit="entries">100</heap>
			<!-- <offheap unit="MB">10</offheap> -->
		</resources>
	</cache-template>
	<!-- A <cache> that uses the template above by referencing the cache-template's 
		name in the uses-template attribute: -->
	<cache alias="UserCache" uses-template="default">
		<expiry>
			<ttl unit="seconds">600</ttl>
		</expiry>
	</cache>
	<cache alias="RoleCache" uses-template="default">
		<expiry>
			<ttl unit="seconds">600</ttl>
		</expiry>
	</cache>
	<cache alias="TcSysCodeListCache" uses-template="default" />
	<cache alias="TmSysParameterCache" uses-template="default" />
	<cache alias="TmBasPartCache" uses-template="default" />
	<cache alias="TrBasPartSupplCache" uses-template="default" />
	<cache alias="TmBasBatchCache" uses-template="default" />
	<cache alias="TmBasSupplResptimeCache" uses-template="default" />
	<cache alias="TmBasStorageRuleCache" uses-template="default" />
	<cache alias="TrBasPartStorageCache" uses-template="default" />
	<cache alias="TrBasPartSupplQuotaCache" uses-template="default" />
	<cache alias="TmBasPlantCache" uses-template="default" />
	<cache alias="TmBasStorageCache" uses-template="default" />
	<cache alias="TmBasTranscodeCheckCache" uses-template="default" />
	<cache alias="TmBasWorkshopCache" uses-template="default" />
	<cache alias="TmBasRouteNodeCache" uses-template="default" />
	<cache alias="TmBasPackageCache" uses-template="default" />
	<cache alias="TmBasSupplCache" uses-template="default" />
	<!-- <cache alias="TmBasShiftModelcodeCache" uses-template="default" /> -->
</config>